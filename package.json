{
  "name": "puppeteer-proxy",
  "version": "1.0.0",
  "description": "A simple proxy browser using Puppeteer",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "express": "^4.18.2",
    "puppeteer-core": "^21.5.0",
    "cors": "^2.8.5",
    "@sparticuz/chromium": "^119.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "postinstall": "echo 'Skipping Chromium download'"
  },
  "config": {
    "puppeteer_skip_chromium_download": "true"
  }
}